{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/formo-cheeses-cheeseblock",
  "version": "0.1.0",
  "title": "Formo Cheesepage Cheeseblock",
  "category": "widgets",
  "icon": "smiley",
  "description": "Displays a product on the cheeses page.",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "source": "html",
      "selector": "h2"
    },
    "layout": {
      "type": "string",
      "default": "text_left"
    },
    "imageID": {
      "type": "string",
      "default": "",
      "source": "attribute",
      "selector": "img",
      "attribute": "data-id"
    },
    "imageUrl": {
      "type": "string",
      "default": "",
      "source": "attribute",
      "selector": ".formo-cheeses-cheeseblock--bg-image img",
      "attribute": "src"
    }
  },
  "textdomain": "formo-cheeses-cheeseblock",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}